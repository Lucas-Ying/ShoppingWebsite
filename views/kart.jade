    include header

block sidebar
    include sidebar

block content
    #RightPart
        #Page
            #Page_center
                table#itemTable.itemTable
                    tr.tableHeader
                        td Product
                        td Price
                        td Quantity
                        td Total
                        td#remove.remove
                p#btn.btn
