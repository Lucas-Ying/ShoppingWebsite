                                a.menu_item(href='mjeans.html')
                                    span Jeans
                                br
                                a.menu_item(href='mtshirts.html')
                                    span
                                        | T-shirts
                                br
                                a.menu_item(href='mshoe.html')
                                    span Footwear
                                br
                                a.menu_item(href='mshorts.html')
                                    span Shorts
                                br
                                a.menu_item(href='mwatches.html')
                                    span
                                        | Watches
                                br
                            .menu_header_left
                                span.menu_text
                                    a(href='women1.html') WOMEN
                            .menu_header_right
                            #Menu_content
                                a.menu_item(href='dress.php?catg=2 & subcatg=dresses')
                                    span Dresses
