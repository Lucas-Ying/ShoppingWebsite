extends layout

block content
    #WholePage
        #Inner
            #Container(style='border:groove;border-color:#00CCFF')
                #Head
                    #Head_left
                        #Leaf_top
                            img(src='img/shop3.jpg', width='324')
                        #Leaf_bottom
                            a#registration.registration
                            a#log-in.log-in
                    #Head_right
                        #Logo
                            #Name.Name
                                span.blue F
                                span ashion
                                span.blue S
                                span hop
