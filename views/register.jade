                                .menu_header_left
                                    span.menu_text
                                        font(face='Georgia, Times New Roman, Times, serif') Search
                                .menu_header_right
                                #Menu_content
                                    h5  
                                    input(type='text', name='t1', value='search', onfocus="if(this.value=='search')\
                        {this.value='';}\
                        ", onblur="if(this.value=='')\
                        {this.value='search';}\
                        ")
                                    input#sub.button(name='sub', type='submit', value='Go')
